{
    "name": "spatie/phpstan-module-boundaries",
    "description": "This is my package phpstan-module-boundaries",
    "keywords": [
        "spatie",
        "phpstan-module-boundaries"
    ],
    "homepage": "https://github.com/spatie/phpstan-module-boundaries",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "phpstan/phpstan": "^1.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\PhpStanModuleBoundaries\\": "src",
            "App\\": "example"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\PhpStanModuleBoundaries\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
